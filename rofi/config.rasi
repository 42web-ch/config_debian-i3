configuration {
font: "mono 18";
show-icons: true;
max-history-size: 6;

  timeout {
      action: "kb-cancel";
      delay:  0;
  }
  filebrowser {
      directories-first: true;
      sorting-method:    "name";
  }
}

@theme "current.rasi"
